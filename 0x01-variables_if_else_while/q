#include <stdio.h>

/**
 * main - Entry point
 *
 * Description: Prints all single-digit numbers of base 10
 * starting from 0 using putchar, followed by a new line.
 *
 * Return: Always 0 (Success)
 */
int main(void)
{
	int i = 0;

	while (i < 10)
	{
		char number = '0' + i;

		putchar(number);

		i++;
	}

	putchar('\n');

	return (0);
}
